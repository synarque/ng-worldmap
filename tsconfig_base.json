{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "declaration": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "sourceRoot": "/",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "typeRoots": [
      "./node_modules/@types/",
      "./typings/"
    ],
    "paths": {
      "@angular/*": ["../../../node_modules/@angular/*"],
      "rxjs/*": ["../../../node_modules/rxjs/*"],
      "ng-worldmap": ["../../../dist/ng-worldmap"]
    }
  }
}